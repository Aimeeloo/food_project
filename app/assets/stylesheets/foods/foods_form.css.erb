.create_form{
  padding: 20px;
  text-align: center;
  <%# background-image: url(/assets/header04.jpg); -------更改成下方格式 %>
  background-image: url(<%= asset_path 'header04.jpg' %>);
}
.container_form{
  background-color: #fff;
}
.row_form{
  padding: 30px 0 100px 0;
}
.col_form{
  width: 100%;
  margin: auto;
  background-color: #fff;
}
.label_form{
  font-size: 18px;
}
.input_img{
  width: 100%;
  font-size: 18px;
  color: #aaa;
  border-radius: 10px;
}
.input_form{
  width: 100%;
  font-size: 18px;
  padding: 5px;
  outline: none;
  padding-left: 15px;
  border-radius: 5px;
  border: 2px solid #ccc;
  background-color: #fff;
}
div.select_form{
  width: 100%;
  font-size: 18px;
  outline: none;
  padding: 5px;
  padding-left: 10px;
  border-radius: 5px;
  border: 2px solid #ccc;
  background-color: #fff;
}
textarea{
  width: 100%;
  outline: none;
  padding: 5px;
  padding-left: 15px;
  border-radius: 5px;
  border: 2px solid #ccc;
  background-color: #fff;
}
.select_form > select{
  width: 100%;
  outline: none;
  padding-left: 10px;
  border: none;
  background-color: #fff;
}
.btn_form{
  width: 120px;
  height: 40px;
  border-radius: 5px;
}